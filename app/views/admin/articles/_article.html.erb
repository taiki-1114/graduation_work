<div id="<%= dom_id article %>">
    <tbody>
      <tr>
        <td><%= article.title%></td>
        <td><%= article.question_body%></td>
        <td><%= article.question_answer %></td>
        <td><%= article.created_at.strftime("%Y-%m-%d") %></td>
        <td><%= article.updated_at.strftime("%Y-%m-%d") %></td>
        <td>
        <%= link_to "詳細", admin_article_path(article), class: "btn btn-primary"%>
        </td>
      </tr>
      <!-- Add more rows for other articles -->
    </tbody>
</div>